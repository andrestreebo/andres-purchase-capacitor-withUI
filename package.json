{
  "name": "@revenuecat/purchases-ui-capacitor",
  "version": "0.1.0",
  "description": "RevenueCat Purchase UI SDK for Capacitor",
  "main": "dist/plugin.cjs.js",
  "module": "dist/esm/index.js",
  "types": "dist/esm/index.d.ts",
  "unpkg": "dist/plugin.js",
  "files": [
    "android/src/main/",
    "android/build.gradle",
    "dist/",
    "ios/Plugin/",
    "RevenuecatPurchasesUiCapacitor.podspec"
  ],
  "author": "RevenueCat",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/andrestreebo/andres-purchase-capacitor-withUI.git"
  },
  "bugs": {
    "url": "https://github.com/andrestreebo/andres-purchase-capacitor-withUI/issues"
  },
  "keywords": [
    "capacitor",
    "plugin",
    "native",
    "revenuecat",
    "purchases",
    "ui"
  ],
  "scripts": {
    "verify": "npm run verify:ios && npm run verify:android && npm run verify:web",
    "verify:ios": "cd ios && pod install && xcodebuild -workspace Plugin.xcworkspace -scheme Plugin -destination generic/platform=iOS && cd ..",
    "verify:android": "cd android && ./gradlew clean build test && cd ..",
    "verify:web": "npm run build",
    "lint": "npm run eslint && npm run prettier -- --check",
    "fmt": "npm run eslint -- --fix && npm run prettier -- --write",
    "eslint": "eslint . --ext ts",
    "prettier": "prettier \"**/*.{css,html,ts,js,java}\"",
    "build": "npm run clean && tsc && rollup -c rollup.config.js",
    "clean": "rimraf ./dist",
    "watch": "tsc --watch",
    "prepublishOnly": "npm run build"
  },
  "dependencies": {
    "@capacitor/core": "^5.0.0"
  },
  "peerDependencies": {
    "@revenuecat/purchases-capacitor": "^10.0.0"
  },
  "devDependencies": {
    "@capacitor/android": "^5.0.0",
    "@capacitor/ios": "^5.0.0",
    "@revenuecat/purchases-capacitor": "^10.0.0",
    "@types/node": "^16.0.0",
    "eslint": "^8.0.0",
    "prettier": "^2.0.0",
    "prettier-plugin-java": "^1.0.0",
    "rimraf": "^3.0.0",
    "rollup": "^2.0.0",
    "typescript": "^4.0.0"
  },
  "capacitor": {
    "ios": {
      "src": "ios"
    },
    "android": {
      "src": "android"
    }
  }
} 